{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Launch TS Program",
          "args": [
            "${workspaceRoot}/src/index.ts" // 入口文件
          ],
          "program": "${workspaceRoot}/node_modules/.bin/ts-node",
          "sourceMaps": true,
          "outFiles": ["${workspaceFolder}/**/*.js"],
          "cwd": "${workspaceRoot}",
          "protocol": "inspector"
      }
  ]
}
